{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 500,
    "max": 1000
  },
  "twillio": {
    "appId": "TWILLIO_APP_ID=",
    "apiKey": "TWILLIO_API_KEY"
  },
  "authentication": {
    "secret":
      "",
    "strategies": ["facebookTokenTeacher", "jwt", "local"],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "100d"
    },
    "local": {
      "entity": "user",
      "service": "users",
      "usernameField": "phone",
      "passwordField": "password",
      "entityUsernameField": "phone",
      "entityPasswordField": "password",
      "passReqToCallback": "true"
    },
    "facebookTokenTeacher": {
      "idField": "facebookId",
      "entity": "user",
      "service": "users",
      "clientID": "FACEBOOK_TEACHER_APP_ID",
      "clientSecret": "FACEBOOK_TEACHER_APP_SECRET",
      "scope": ["public_profile", "email"],
      "profileFields": [
        "id",
        "displayName",
        "first_name",
        "last_name",
        "email",
        "gender",
        "profileUrl",
        "birthday",
        "picture",
        "permissions"
      ]
    },
    "cookie": {
      "enabled": true,
      "name": "learnla",
      "httpOnly": false,
      "secure": false
    }
  },
  "mongodb": "mongodb://localhost:27017/learn_la"
}
